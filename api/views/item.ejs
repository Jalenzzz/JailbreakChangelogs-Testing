<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="referrer" content="no-referrer" />

    <!-- Primary Meta Tags -->
    <title><%= item.name %> - Roblox Jailbreak</title>
    <meta name="title" content="<%= item.name %> - Roblox Jailbreak" />
    <meta
      name="description"
      content="<%= (item.description && item.description !== 'N/A') ? item.description : `Get the latest value and details for ${item.name} in Roblox Jailbreak. View current price, trading history, rarity status, and market trends.` %>"
    />
    <meta
      name="keywords"
      content="<%= item.name.toLowerCase() %> jailbreak, 
      jailbreak <%= item.name.toLowerCase() %> value, 
      <%= itemType.toLowerCase() %> worth jailbreak,
      jailbreak trading values,
      roblox jailbreak <%= itemType %>,
      jailbreak <%= item.name.toLowerCase() %> price,
      <%= item.name.toLowerCase() %> trading value,
      jb trading <%= item.name.toLowerCase() %>,
      jb value <%= item.name.toLowerCase() %>,
      what is <%= item.name.toLowerCase() %> worth in jailbreak,
      how much is <%= item.name.toLowerCase() %> worth in jailbreak,
      <%= item.name.toLowerCase() %> jb price,
      <%= item.name.toLowerCase() %> trading guide,
      <%= item.name.toLowerCase() %> value list,
      <%= item.name.toLowerCase() %> trade value jailbreak,
      <%= item.name.toLowerCase() %> price guide,
      <%= item.name.toLowerCase() %> value checker,
      <%= item.name.toLowerCase() %> trading price,
      <%= item.name.toLowerCase() %> worth calculator,
      <%= item.name.toLowerCase() %> rarity jailbreak,
      <%= item.name.toLowerCase() %> demand value,
      <%= item.name.toLowerCase() %> market price,
      <%= item.name.toLowerCase() %> current value,
      jailbreak <%= itemType.toLowerCase() %> <%= item.name.toLowerCase() %>,
      roblox jb <%= item.name.toLowerCase() %>"
    />

    <meta name="author" content="Jailbreak Changelogs" />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="product" />
    <meta
      property="og:url"
      content="https://jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(item.name) %>"
    />
    <meta property="og:title" content="<%= item.name %> - Roblox Jailbreak" />
    <meta
      property="og:description"
      content="<%= (item.description && item.description !== 'N/A') ? item.description : `Get the latest trading value and details for ${item.name} in Roblox Jailbreak. Updated ${itemType.toLowerCase()} values, rarity info, and market trends.` %>"
    />
    <meta property="og:image" content="<%= embedImageUrl %>" />

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta
      name="twitter:url"
      content="https://jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(item.name) %>"
    />
    <meta name="twitter:title" content="<%= item.name %> - Roblox Jailbreak" />
    <meta
      name="twitter:description"
      content="<%= (item.description && item.description !== 'N/A') ? item.description : `Get the latest trading value and details for ${item.name} in Roblox Jailbreak. Updated ${itemType.toLowerCase()} values, rarity info, and market trends.` %>"
    />
    <meta property="twitter:image" content="<%= embedImageUrl %>" />

    <!-- Additional SEO tags -->
    <meta
      name="robots"
      content="index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
    />
    <meta
      property="article:modified_time"
      content="<%= new Date().toISOString() %>"
    />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="article" />

    <!-- Additional SEO Meta Tags -->
    <meta name="twitter:label1" content="Item Type" />
    <meta name="twitter:data1" content="<%= itemType %>" />
    <meta name="twitter:label2" content="Current Value" />
    <meta name="twitter:data2" content="<%= item.cash_value %>" />

    <!-- Enhanced Keywords -->
    <meta
      name="keywords"
      content="<%= item.name.toLowerCase() %> jailbreak value, jailbreak <%= item.name.toLowerCase() %>, <%= item.name.toLowerCase() %> worth, jailbreak trading value <%= item.name.toLowerCase() %>, <%= item.name.toLowerCase() %> price jailbreak, roblox jailbreak <%= item.name.toLowerCase() %>, jailbreak <%= itemType.toLowerCase() %> values"
    />

    <meta name="robots" content="index, follow, max-image-preview:large" />
    <meta name="theme-color" content="#124e66" />
    <link
      rel="canonical"
      href="https://jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(item.name) %>"
    />

    <!-- Schema.org structured data for Game Item -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "Game Item",
        "name": "<%= item.name %>",
        "description": "<%- (item.description && item.description !== 'N/A') ? item.description : `${item.name} is a ${itemType.toLowerCase()} item in Roblox Jailbreak.` %>",
        "image": "<%= item.image || logoUrl %>",
        "additionalType": "VideoGameItem",
        "additionalProperty": [
          {
            "@type": "PropertyValue",
            "name": "Game",
            "value": "Roblox Jailbreak"
          },
          {
            "@type": "PropertyValue",
            "name": "ItemType",
            "value": "<%= itemType %>"
          },
          {
            "@type": "PropertyValue",
            "name": "InGameValue",
            "value": "<%= item.cash_value %>"
          },
          {
            "@type": "PropertyValue",
            "name": "Availability",
            "value": "<%- item.is_limited ? 'Limited' : 'Regular' %>"
          }
        ]
      }
    </script>

    <!-- Schema.org structured data for Breadcrumbs -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "BreadcrumbList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "name": "Home",
            "item": "https://jailbreakchangelogs.xyz"
          },
          {
            "@type": "ListItem",
            "position": 2,
            "name": "Values",
            "item": "https://jailbreakchangelogs.xyz/values"
          },
          {
            "@type": "ListItem",
            "position": 3,
            "name": "<%= itemType %>s",
            "item": "https://jailbreakchangelogs/values?sort=<%= itemType.toLowerCase() %>s"
          },
          {
            "@type": "ListItem",
            "position": 4,
            "name": "<%= item.name %>",
            "item": "https://jailbreakchangelogs/item/<%= itemType %>/<%= encodeURIComponent(item.name) %>"
          }
        ]
      }
    </script>

    <!-- Additional Structured Data -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": [
          {
            "@type": "Question",
            "name": "What is the value of <%= item.name %> in Jailbreak?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "The current cash value of <%= item.name %> is <%= item.cash_value %>. <% if(item.is_limited) { %>This is a limited item, which means it is rare and potentially more valuable.<% } %>"
            }
          },
          {
            "@type": "Question",
            "name": "Is <%= item.name %> a limited item in Jailbreak?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "<% if(item.is_limited) { %>Yes, <%= item.name %> is a limited item in Jailbreak, making it more valuable and harder to obtain.<% } else { %>No, <%= item.name %> is not a limited item in Jailbreak.<% } %>"
            }
          }
        ]
      }
    </script>

    <base href="/" />

    <!-- Stylesheets -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/colors.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.34/styles/item.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/comments.min.css"
    />
    <!-- <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/comments.min.css"
    /> -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/main.min.css"
    />

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Luckiest+Guy&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/notyf@3.10.0/notyf.min.css"
      integrity="sha256-IwkvZNRC/3S26O1gWwjBINmrPZ4zYvPX4z/98OKWHkQ="
      crossorigin="anonymous"
    />
  </head>

  <body data-scroll-container>
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-KM853GJ6"
        height="0"
        width="0"
        style="display: none; visibility: hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- prettier-ignore -->
    <%- include('partials/nav') %>
    <%- include('partials/loading') %>

    <div class="container-fluid mt-3">
      <!-- Breadcrumb moved to top -->
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item"><a href="/values">Values</a></li>
          <li class="breadcrumb-item">
            <a
              href="/values?sort=<%= itemType.toLowerCase() %>s&valueSort=cash-desc"
            >
              <%= itemType.charAt(0).toUpperCase() + itemType.slice(1) %>s
            </a>
          </li>
          <li class="breadcrumb-item active" aria-current="page">
            <%= item.name %>
          </li>
        </ol>
      </nav>

      <!-- Tabs Navigation -->
      <ul
        class="nav nav-tabs nav-fill justify-content-center mb-0"
        id="itemTabs"
        role="tablist"
      >
        <li class="nav-item" role="presentation">
          <button
            class="nav-link active"
            id="details-tab"
            data-bs-toggle="tab"
            data-bs-target="#details"
            type="button"
            role="tab"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="18"
              height="18"
              viewBox="0 0 24 24"
            >
              <rect width="24" height="24" fill="none" />
              <path
                fill="currentColor"
                d="M11 17h2v-6h-2zm1-8q.425 0 .713-.288T13 8t-.288-.712T12 7t-.712.288T11 8t.288.713T12 9m0 13q-2.075 0-3.9-.788t-3.175-2.137T2.788 15.9T2 12t.788-3.9t2.137-3.175T8.1 2.788T12 2t3.9.788t3.175 2.137T21.213 8.1T22 12t-.788 3.9t-2.137 3.175t-3.175 2.138T12 22"
              />
            </svg>
            Details
          </button>
        </li>
        <li class="nav-item" role="presentation">
          <button
            class="nav-link"
            id="similar-tab"
            data-bs-toggle="tab"
            data-bs-target="#similar"
            type="button"
            role="tab"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="18"
              height="18"
              viewBox="0 0 24 24"
            >
              <rect width="24" height="24" fill="none" />
              <g fill="none">
                <path
                  fill="currentColor"
                  fill-opacity="0.16"
                  d="M8.4 3H4.6A1.6 1.6 0 0 0 3 4.6v3.8A1.6 1.6 0 0 0 4.6 10h3.8A1.6 1.6 0 0 0 10 8.4V4.6A1.6 1.6 0 0 0 8.4 3m11 11h-3.8a1.6 1.6 0 0 0-1.6 1.6v3.8a1.6 1.6 0 0 0 1.6 1.6h3.8a1.6 1.6 0 0 0 1.6-1.6v-3.8a1.6 1.6 0 0 0-1.6-1.6"
                />
                <path
                  stroke="currentColor"
                  stroke-linejoin="round"
                  stroke-miterlimit="10"
                  stroke-width="1.5"
                  d="M8.4 3H4.6A1.6 1.6 0 0 0 3 4.6v3.8A1.6 1.6 0 0 0 4.6 10h3.8A1.6 1.6 0 0 0 10 8.4V4.6A1.6 1.6 0 0 0 8.4 3Zm11 0h-3.8A1.6 1.6 0 0 0 14 4.6v3.8a1.6 1.6 0 0 0 1.6 1.6h3.8A1.6 1.6 0 0 0 21 8.4V4.6A1.6 1.6 0 0 0 19.4 3Zm-11 11H4.6A1.6 1.6 0 0 0 3 15.6v3.8A1.6 1.6 0 0 0 4.6 21h3.8a1.6 1.6 0 0 0 1.6-1.6v-3.8A1.6 1.6 0 0 0 8.4 14Zm11 0h-3.8a1.6 1.6 0 0 0-1.6 1.6v3.8a1.6 1.6 0 0 0 1.6 1.6h3.8a1.6 1.6 0 0 0 1.6-1.6v-3.8a1.6 1.6 0 0 0-1.6-1.6Z"
                />
              </g>
            </svg>
            Similar Items
          </button>
        </li>
        <li class="nav-item" role="presentation">
          <button
            class="nav-link"
            id="comments-tab"
            data-bs-toggle="tab"
            data-bs-target="#comments"
            type="button"
            role="tab"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="18"
              height="18"
              viewBox="0 0 16 16"
            >
              <rect width="16" height="16" fill="none" />
              <path
                fill="currentColor"
                fill-rule="evenodd"
                d="M2 0a2 2 0 0 0-2 2v10.06l1.28-1.28l1.53-1.53H4V11a2 2 0 0 0 2 2h7l1.5 1.5L16 16V6a2 2 0 0 0-2-2h-2V2a2 2 0 0 0-2-2zm8.5 4V2a.5.5 0 0 0-.5-.5H2a.5.5 0 0 0-.5.5v6.44l.47-.47l.22-.22H4V6a2 2 0 0 1 2-2zm3.56 7.94l.44.439V6a.5.5 0 0 0-.5-.5H6a.5.5 0 0 0-.5.5v5a.5.5 0 0 0 .5.5h7.621z"
                clip-rule="evenodd"
              />
            </svg>
            Comments
          </button>
        </li>
      </ul>

      <!-- Tabs Content -->
      <div class="tab-content" id="itemTabsContent">
        <!-- Details Tab -->
        <div class="tab-pane fade show active" id="details" role="tabpanel">
          <div id="item-container">
            <!-- Item details will be loaded here -->
          </div>
        </div>

        <!-- Similar Items Tab -->
        <div class="tab-pane fade" id="similar" role="tabpanel">
          <div class="container">
            <h3
              class="mb-4"
              style="font-family: 'Luckiest Guy', cursive; letter-spacing: 1px"
            >
              Similar Items <span class="text-muted">(<%= itemType %>s)</span>
            </h3>
            <div class="row g-4" id="similar-items">
              <!-- Similar items will be loaded here -->
            </div>
          </div>
        </div>

        <!-- Comments Tab -->
        <div class="tab-pane fade" id="comments" role="tabpanel">
          <div class="container">
            <%- include('partials/comments', { type: itemType.toLowerCase(),
            itemId: item.id, itemName: item.name }) %>
          </div>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>

    <!-- Scripts -->
    <script
      src="https://code.jquery.com/jquery-3.7.1.min.js"
      integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo="
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/notyf@3.10.0/notyf.min.js"
      integrity="sha256-UnlpkMLasaTx2ZqovxBXUcQ5jq3oKXaZZ1aWENNFETE="
      crossorigin="anonymous"
    ></script>
    <script src="https://code.highcharts.com/stock/highstock.js"></script>
    <script src="https://code.highcharts.com/modules/full-screen.js"></script>
    <script src="https://code.highcharts.com/modules/accessibility.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.34/scripts/item.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.29/scripts/comments.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.29/scripts/main.min.js"></script>
  </body>
</html>
