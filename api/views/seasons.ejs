<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="referrer" content="no-referrer" />

    <base href="/" />

    <!-- Primary Meta Tags -->
    <meta
      name="title"
      content="Jailbreak Season <%= season %> | <%= title %>"
    />
    <meta name="author" content="Jailbreak Changelogs" />
    <meta
      name="description"
      content="Explore Jailbreak Season <%= season %>: <%= title %>. Track seasonal rewards, exclusive vehicles, limited-time items, and progression milestones for Roblox Jailbreak Season <%= season %>."
    />
    <meta
      name="keywords"
      content="Jailbreak Season <%= season %>, <%= title %>, jb wiki seasons, jailbreak wiki seasons, Roblox Jailbreak, seasonal rewards, exclusive vehicles, limited items, game progression, seasonal content, Jailbreak updates, seasonal milestones, game events"
    />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="Jailbreak Changelogs" />
    <meta
      property="og:url"
      content="https://jailbreakchangelogs.xyz/seasons/<%= season %>"
    />
    <meta
      property="og:title"
      content="Jailbreak Season <%= season %> | <%= title %>"
    />
    <meta
      property="og:description"
      content="Explore Jailbreak Season <%= season %>: <%= title %>. Track seasonal rewards, unlock exclusive items, and discover all rewards in this limited-time event!"
    />
    <% image_urls.forEach(function(url) { %>
    <meta property="og:image" content="<%= url %>" />
    <% }); %>
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="630" />

    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta
      name="twitter:url"
      content="https://jailbreakchangelogs.xyz/seasons/<%= season %>"
    />
    <meta
      name="twitter:title"
      content="Jailbreak Season <%= season %> | <%= title %>"
    />
    <meta
      name="twitter:description"
      content="Explore Jailbreak Season <%= season %>: <%= title %>. Track seasonal rewards, unlock exclusive items, and discover all rewards in this limited-time event!"
    />
    <% image_urls.forEach(function(url) { %>
    <meta name="twitter:image" content="<%= url %>" />
    <% }); %>

    <!-- Additional SEO -->
    <meta name="robots" content="index, follow" />
    <meta name="theme-color" content="#124e66" />
    <meta name="mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />

    <!-- Canonical URL -->
    <link
      rel="canonical"
      href="https://jailbreakchangelogs.xyz/seasons/<%= season %>"
    />

    <title>Season <%= season %> - Changelogs</title>

    <!-- Stylesheets -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/colors.min.css"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/main.min.css"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/seasons.min.css"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/styles/comments.min.css"
    />

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Bangers&family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&family=Luckiest+Guy&family=Merriweather+Sans:ital,wght@0,300..800;1,300..800&family=Montserrat:ital,wght@0,100..900;1,100..900&family=Oswald:wght@200..700&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/notyf@3.10.0/notyf.min.css"
      integrity="sha256-IwkvZNRC/3S26O1gWwjBINmrPZ4zYvPX4z/98OKWHkQ="
      crossorigin="anonymous"
    />
    <!-- Google Tag Manager -->
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ "gtm.start": new Date().getTime(), event: "gtm.js" });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != "dataLayer" ? "&l=" + l : "";
        j.async = true;
        j.src = "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, "script", "dataLayer", "GTM-KM853GJ6");
    </script>
    <!-- End Google Tag Manager -->

    <!-- Additional SEO Questions -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": [
          {
            "@type": "Question",
            "name": "When does Jailbreak Season <%= season %> end?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Jailbreak Season <%= season %> ends on February 25, 2025. Check our countdown timer above for the exact time remaining. New seasons typically start shortly after the previous season ends."
            }
          },
          {
            "@type": "Question",
            "name": "What rewards are available in Jailbreak Season <%= season %>?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Season <%= season %> features exclusive rewards including limited-time vehicles, items, and customizations. View the complete list of rewards and their level requirements on our season details page."
            }
          }
        ]
      }
    </script>
  </head>
  <!-- prettier-ignore -->
  <%- include('partials/nav') %>
  <%- include('partials/loading') %>

  <body data-scroll-container>
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-KM853GJ6"
        height="0"
        width="0"
        style="display: none; visibility: hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->

    <!-- Seasons Button Container -->
    <div class="container-fluid mt-3">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item"><a href="/seasons">Seasons</a></li>
          <li
            class="breadcrumb-item active season-breadcrumb"
            aria-current="page"
          ></li>
        </ol>
      </nav>

      <!-- New Seasons Introduction Section -->
      <div class="seasons-intro mb-4">
        <h1 class="display-4 mb-3">Roblox Jailbreak Season Archives</h1>
        <div class="row">
          <div class="col-md-12">
            <div class="intro-text">
              <p class="lead">
                Explore every season of Roblox Jailbreak! Each season brings
                exciting limited-time rewards, exclusive vehicles, and unique
                customization items. Level up, earn XP, and unlock special
                prizes during these time-limited events.
              </p>

              <!-- Keep only the new countdowns container -->
              <div class="countdowns-container row mt-4">
                <!-- Current Season Countdown -->
                <div class="col-12 col-md-6 mb-3">
                  <div id="current-season-countdown" class="countdown-wrapper">
                    <!-- Current season countdown will be inserted here -->
                  </div>
                </div>

                <!-- Next Season Countdown -->
                <div class="col-12 col-md-6 mb-3">
                  <div id="next-season-countdown" class="countdown-wrapper">
                    <!-- Next season countdown will be inserted here -->
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-12 col-lg-8">
          <!-- This aligns with the content wrapper width -->
          <div class="row">
            <div class="col-auto">
              <div class="nav-item dropdown">
                <a
                  class="nav-link dropdown-toggle btn btn-primary custom-dropdown-btn"
                  href="#"
                  id="seasonsDropdown"
                  role="button"
                  data-bs-toggle="dropdown"
                  aria-expanded="false"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 24 24"
                  >
                    <rect width="24" height="24" fill="none" />
                    <path
                      fill="currentColor"
                      d="M17 3h4a1 1 0 0 1 1 1v16a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V4a1 1 0 0 1 1-1h4V1h2v2h6V1h2zM4 9v10h16V9zm2 4h5v4H6z"
                    />
                  </svg>
                  View Seasons
                </a>
                <ul
                  class="dropdown-menu changelog-dropdown w-100"
                  id="seasonList"
                  aria-labelledby="seasonsDropdown"
                >
                  <!-- Dropdown items will be populated dynamically by JavaScript -->
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Main Content Container -->
    <div id="main-container" class="container-fluid">
      <div id="content-wrapper" class="row mt-3">
        <!-- Season Details Section -->
        <section id="content" class="d-flex flex-grow-1">
          <div id="season-details" class="col-12">
            <!-- Season description and rewards will be dynamically inserted here by JavaScript -->
          </div>
        </section>

        <!-- Rewards Carousel Section -->
        <div class="col-12 col-sm-4 col-md-4" id="sidebar-image-wrapper">
          <!-- Carousel Image -->
          <div
            id="rewardsCarousel"
            class="carousel slide"
            data-bs-ride="carousel"
          >
            <div class="carousel-inner" id="carousel-inner">
              <!-- Carousel items will be dynamically inserted here by JavaScript -->
            </div>
            <!-- Carousel Navigation Buttons -->
            <button
              class="carousel-control-prev"
              type="button"
              data-bs-target="#rewardsCarousel"
              data-bs-slide="prev"
            >
              <span
                class="carousel-control-prev-icon"
                aria-hidden="true"
              ></span>
              <span class="visually-hidden">Previous</span>
            </button>
            <button
              class="carousel-control-next"
              type="button"
              data-bs-target="#rewardsCarousel"
              data-bs-slide="next"
            >
              <span
                class="carousel-control-next-icon"
                aria-hidden="true"
              ></span>
              <span class="visually-hidden">Next</span>
            </button>
          </div>

          <%- include('partials/comments', { type: 'season', itemId: seasonId })
          %>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>
    <!-- jQuery -->
    <script
      src="https://code.jquery.com/jquery-3.7.1.min.js"
      integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo="
      crossorigin="anonymous"
    ></script>

    <!-- Bootstrap Bundle -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>

    <script
      src="https://cdn.jsdelivr.net/npm/notyf@3.10.0/notyf.min.js"
      integrity="sha256-UnlpkMLasaTx2ZqovxBXUcQ5jq3oKXaZZ1aWENNFETE="
      crossorigin="anonymous"
    ></script>

    <!-- Custom scripts -->
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.27/scripts/main.min.js"></script>
    <!-- <script src="scripts/main.js"></script> -->
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/scripts/comments.min.js"></script>
    <!-- <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.14/scripts/comments.min.js"></script> -->
    <!-- <script src="scripts/seasons.js"></script> -->
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.1.26/scripts/seasons.min.js"></script>

    <script
      async
      src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8152532464536367"
      crossorigin="anonymous"
    ></script>
    <script
      async
      src="https://fundingchoicesmessages.google.com/i/pub-8152532464536367?ers=1"
    ></script>
    <script>
      (function () {
        function signalGooglefcPresent() {
          if (!window.frames["googlefcPresent"]) {
            if (document.body) {
              const iframe = document.createElement("iframe");
              iframe.style =
                "width: 0; height: 0; border: none; z-index: -1000; left: -1000px; top: -1000px;";
              iframe.style.display = "none";
              iframe.name = "googlefcPresent";
              document.body.appendChild(iframe);
            } else {
              setTimeout(signalGooglefcPresent, 0);
            }
          }
        }
        signalGooglefcPresent();
      })();
    </script>
  </body>
</html>
