  <body>
    <!-- prettier-ignore -->
    <%- include('partials/nav') %>
    <%- include('partials/loading') %>
    <div class="container mt-4">
      <!-- Breadcrumbs -->
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <!-- Will be populated by JavaScript -->
        </ol>
      </nav>

      <!-- Main content container -->
      <div class="trade-content">
        <!-- Trade Details -->
        <div class="trade-ad-detail">
          <div class="trade-ad">
            <!-- Author Info -->
            <div class="trader-info">
              <!-- Will be populated by JavaScript -->
            </div>

            <!-- Trade Items -->
            <div class="trade-sides-container">
              <!-- Offering Side -->
              <div class="trade-side offering">
                <div class="trade-side-label">Offering</div>
                <div class="trade-items-grid">
                  <!-- Will be populated by JavaScript -->
                </div>
                <div class="side-total">
                  <!-- Will be populated by JavaScript -->
                </div>
              </div>

              <!-- Requesting Side -->
              <div class="trade-side requesting">
                <div class="trade-side-label">Requesting</div>
                <div class="trade-items-grid">
                  <!-- Will be populated by JavaScript -->
                </div>
                <div class="side-total">
                  <!-- Will be populated by JavaScript -->
                </div>
              </div>
            </div>

            <!-- Trade Footer -->
            <div class="trade-ad-footer">
              <div class="trade-timestamp">
                <!-- <i class="bi bi-clock"></i> -->
                <span class="timestamp">
                  <!-- Will be populated by JavaScript -->
                </span>
              </div>
              <div class="trade-status">
                <!-- Will be populated by JavaScript -->
              </div>
            </div>

            <div class="trade-actions mt-3">
              <!-- Will be populated by JavaScript -->
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="container">
      <%- include('partials/comments', { type: 'trade', itemId: tradeId }) %>
    </div>
    <%- include('partials/footer') %>

    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
