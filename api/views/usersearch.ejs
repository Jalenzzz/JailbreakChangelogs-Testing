    <!-- Updated meta description for the users page -->
    <meta
      name="description"
      content="Search for users on Jailbreak Changelogs and manage your own profile. Engage with the community through comments and track your contributions!"
    />

    <!-- Open Graph metadata for social media sharing -->
    <meta property="og:title" content="Jailbreak Changelogs | User Search" />
    <meta
      property="og:description"
      content="Search for users on Jailbreak Changelogs and manage your own profile. Engage with the community through comments and track your contributions!"
    />
    <meta property="og:image" content="<%= logoUrl %>" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="630" />
    <meta property="og:url" content="https://jailbreakchangelogs.xyz/users" />
    <meta property="og:type" content="website" />

    <!-- Metadata for SEO -->
    <meta name="robots" content="index, follow" />
    <meta name="theme-color" content="#124e66" />

    <meta
      name="keywords"
      content="Jailbreak users, user profiles, community members, contributions, comments, user statistics, profile management, user interactions, Roblox Jailbreak community"
    />

    <!-- Twitter Card metadata -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="Jailbreak Changelogs | User Search" />
    <meta
      name="twitter:description"
      content="Search for users on Jailbreak Changelogs and manage your own profile. Engage with the community through comments and track your contributions!"
    />
    <meta property="twitter:image" content="<%= logoUrl %>" />

    <title><%= title %></title>
    <link rel="canonical" href="https://jailbreakchangelogs.xyz/users" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4Q6Gf2aSP4eDXB8Miphtr37CMZZQ5oXLH2yaXMJ2w8e2ZtHTl7GptT4jmndRuHDT"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/notyf@3.10.0/notyf.min.css"
      integrity="sha256-IwkvZNRC/3S26O1gWwjBINmrPZ4zYvPX4z/98OKWHkQ="
      crossorigin="anonymous"
    />

    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@2.0.0/styles/colors.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@2.0.0/styles/usersearch.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@2.0.0/styles/main.min.css"
    />

    <link rel="icon" href="/favicon.ico" sizes="any">
    <link rel="apple-touch-icon" href="/assets/logos/apple-touch-icon.png" />
    <meta name="apple-mobile-web-app-title" content="Changelogs">
    <meta name="application-name" content="Changelogs">
  <script type="text/javascript">
    (function(c,l,a,r,i,t,y){
        c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
        t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
        y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
