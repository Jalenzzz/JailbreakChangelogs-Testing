  <body>
    <!-- prettier-ignore -->
    <%- include('partials/nav') %>

    <div style="background-color: #2e3944; color: white; padding: 20px 0">
      <div class="container">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">
              User Search
            </li>
          </ol>
        </nav>

        <div class="container px-3">
          <div class="text-center mb-3">
            <h1 class="fs-3 fw-bold">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="32"
                height="32"
                viewBox="0 0 16 16"
              >
                <rect width="16" height="16" fill="none" />
                <path
                  fill="currentColor"
                  d="M7 14s-1 0-1-1s1-4 5-4s5 3 5 4s-1 1-1 1zm4-6a3 3 0 1 0 0-6a3 3 0 0 0 0 6m-5.784 6A2.24 2.24 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.3 6.3 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1zM4.5 8a2.5 2.5 0 1 0 0-5a2.5 2.5 0 0 0 0 5"
                  stroke-width="0.5"
                  stroke="#1d7da3"
                />
              </svg>
              User Search
            </h1>
            <p class="text-muted small mb-2">
              Find users by their username or display name
            </p>
            <p class="text-muted small" id="total-users-count">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="16"
                height="16"
                viewBox="0 0 16 16"
              >
                <rect width="16" height="16" fill="none" />
                <path
                  fill="currentColor"
                  d="M6 8a3 3 0 1 0 0-6a3 3 0 0 0 0 6m-5 6s-1 0-1-1s1-4 6-4s6 3 6 4s-1 1-1 1zM11 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 0 1h-4a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 0 0 1h4a.5.5 0 0 0 0-1zm2 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1zm0 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1z"
                />
              </svg>
              Loading user count...
            </p>
          </div>

          <div class="row justify-content-center">
            <div class="col-12 col-md-8 col-lg-6">
              <div
                class="search-box shadow-sm p-2 mb-3"
                style="background-color: #212a31"
              >
                <div class="input-group">
                  <span class="input-group-text">
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 48 48"
                    >
                      <rect width="48" height="48" fill="none" />
                      <g
                        fill="none"
                        stroke="#748d92"
                        stroke-linejoin="round"
                        stroke-width="4"
                      >
                        <path
                          d="M21 38c9.389 0 17-7.611 17-17S30.389 4 21 4S4 11.611 4 21s7.611 17 17 17Z"
                        />
                        <path
                          stroke-linecap="round"
                          d="M26.657 14.343A7.98 7.98 0 0 0 21 12a7.98 7.98 0 0 0-5.657 2.343m17.879 18.879l8.485 8.485"
                        />
                      </g>
                    </svg>
                  </span>
                  <input
                    type="text"
                    id="searchInput"
                    placeholder="Search for a user..."
                    class="form-control ps-0"
                    autocomplete="off"
                  />
                  <button
                    class="btn btn-outline-secondary"
                    id="clearButton"
                    type="button"
                    style="display: none"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 32 32"
                    >
                      <rect width="32" height="32" fill="none" />
                      <path
                        fill="currentColor"
                        d="M16 2C8.2 2 2 8.2 2 16s6.2 14 14 14s14-6.2 14-14S23.8 2 16 2m5.4 21L16 17.6L10.6 23L9 21.4l5.4-5.4L9 10.6L10.6 9l5.4 5.4L21.4 9l1.6 1.6l-5.4 5.4l5.4 5.4z"
                      />
                    </svg>
                  </button>
                  <button class="btn btn-primary px-3" id="searchButton">
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 24 24"
                    >
                      <rect width="24" height="24" fill="none" />
                      <g
                        fill="none"
                        stroke="currentColor"
                        stroke-linecap="round"
                        stroke-linejoin="round"
                        stroke-width="2"
                      >
                        <path
                          stroke-dasharray="20"
                          stroke-dashoffset="20"
                          d="M3 12h17.5"
                        >
                          <animate
                            fill="freeze"
                            attributeName="stroke-dashoffset"
                            dur="0.2s"
                            values="20;0"
                          />
                        </path>
                        <path
                          stroke-dasharray="12"
                          stroke-dashoffset="12"
                          d="M21 12l-7 7M21 12l-7 -7"
                        >
                          <animate
                            fill="freeze"
                            attributeName="stroke-dashoffset"
                            begin="0.2s"
                            dur="0.2s"
                            values="12;0"
                          />
                        </path>
                      </g>
                    </svg>
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="row justify-content-center">
          <div class="col-12">
            <!-- Removed display: none from results-container -->
            <div
              id="user-results"
              class="results-container fade-in"
              style="background-color: #212a31; padding: 1.5rem"
            >
              <!-- Updated loading spinner -->
              <div id="loading-spinner" class="text-center py-4">
                <div
                  class="spinner-border"
                  style="color: #1d7da3"
                  role="status"
                >
                  <span class="visually-hidden">Loading...</span>
                </div>
              </div>

              <div id="usersGrid" class="user-grid">
                <!-- Users will be populated here -->
              </div>

              <!-- Added pagination controls -->
              <div
                id="pagination-controls"
                class="d-flex justify-content-center mt-4"
              >
                <!-- Pagination will be populated here -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/js/bootstrap.bundle.min.js"
